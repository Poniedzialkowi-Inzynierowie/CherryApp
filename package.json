{
  "name": "cherryapp",
  "version": "0.1.0",
  "description": "Progressive web application made for Technikum Mechatroniczne nr 1 in Warsaw.",
  "main": "",
  "babel": {
    "presets": [
      "react",
      "env"
    ],
    "plugins": [
      [
        "transform-react-jsx",
        {
          "pragma": "h"
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": [
      "standard",
      "standard-preact"
    ],
    "parserOptions": {
      "ecmaVersion": 8
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "NODE_ENV=production node server/main.js",
    "heroku-postbuild": "gulp build --production",
    "lint": "node_modules/.bin/eslint src/**/*.js *.js server/**/*.js",
    "lint-fix": "node_modules/.bin/eslint src/**/*.js *.js server/**/*.js --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Poniedzialkowi-Inzynierowie/CherryApp.git"
  },
  "contributors": [
    "Kacper Skrzynski <adamn.zxc@gmail.com>",
    "Alan Borowy <selfish570@gmail.com>",
    "Jan Szachno <j.szachno@gmail.com>",
    "Piotr Stadnicki <pitstadnicki@gmail.com>"
  ],
  "license": "GPLv3",
  "bugs": {
    "url": "https://github.com/Poniedzialkowi-Inzynierowie/CherryApp/issues"
  },
  "homepage": "https://github.com/Poniedzialkowi-Inzynierowie/CherryApp#readme",
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babelify": "^8.0.0",
    "del": "^3.0.0",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-autoprefixer": "^4.0.0",
    "gulp-browserify": "^0.5.1",
    "gulp-load-plugins": "^1.5.0",
    "gulp-nop": "0.0.3",
    "gulp-notify": "^3.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.6.1",
    "gulp-uglify": "^3.0.0",
    "gulp-watch": "^4.3.11",
    "gulp-webserver": "^0.9.1",
    "preact": "^8.2.6",
    "workbox-build": "^2.1.0"
  },
  "devDependencies": {
    "eslint": "^4.10.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-preact": "^1.1.6",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.4.0",
    "eslint-plugin-standard": "^3.0.1"
  },
  "optionalDependencies": {}
}
